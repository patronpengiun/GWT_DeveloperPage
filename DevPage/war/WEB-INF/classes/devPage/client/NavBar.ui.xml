<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:base="com.github.gwtbootstrap.client.ui.base"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	    .in-btn {margin-right:25px;}
	    .welcome-span {display:inline;}
  	</ui:style>
	
	
	<g:FlowPanel>
	     <b:Navbar>
	     	 <b:NavCollapse>
		         <b:Brand>Developer Page</b:Brand>
		         <b:Nav>
		             <b:Dropdown text="Game Console">
	                 	<b:NavLink ui:field="upload">Submit a Game</b:NavLink>
	                 	<b:NavLink ui:field="update">Update a Game</b:NavLink>
	                 	<b:NavLink ui:field="delete">Delete a Game</b:NavLink>
	            	 </b:Dropdown>	
		             <b:NavLink ui:field="dashboard">Dash Board</b:NavLink>
		             <b:NavLink ui:field="database">Database Query</b:NavLink>
		         </b:Nav>
	         	 <b:NavForm alignment="RIGHT" size="2" ui:field="form">
	         	 	<b:TextBox placeholder="username" alternateSize="SMALL" ui:id="navbar-username" ui:field="name"></b:TextBox>
	         	 	<b:PasswordTextBox placeholder="password" alternateSize="SMALL" ui:id="navbar-password" ui:field="password"></b:PasswordTextBox>
	         	 	<b:Button addStyleNames="{style.in-btn}" ui:field="inBtn">Sign in</b:Button>
	         	 	<b:Button ui:field="upBtn">Sign up</b:Button>
	         	 </b:NavForm>
	         	 <b:NavText ui:field="welcome" alignment="RIGHT" addStyleNames="{style.welcome-span}"></b:NavText>
	         </b:NavCollapse>
	     </b:Navbar>
	 </g:FlowPanel>
 </ui:UiBinder> 